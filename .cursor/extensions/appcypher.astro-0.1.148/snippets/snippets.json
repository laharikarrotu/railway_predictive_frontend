{
    "Function": {
        "prefix": "fun",
        "body": [
            "fun ${1:name}(${2:param}):",
            "    ${3:pass}"
        ],
        "description": "Defines a function with the specified parameters."
    },
    "Variable": {
        "prefix": "var",
        "body": [ "var ${1:variable} = ${2:variable}" ],
        "description": "Defines a function with the specified parameters."
    },
    "Constant": {
        "prefix": "let",
        "body": [ "let ${1:variable} = ${2:variable}" ],
        "description": "Defines a function with the specified parameters."
    },
    "Type": {
        "prefix": "type",
        "body": [
            "type ${1:name}(${2:param}):",
            "    ${3:pass}"
        ],
        "description": "Defines a type with the specified parameters."
    },
    "Print": {
        "prefix": "type",
        "body": [ "print('${1:message}')" ],
        "description": "Creates a print statement."
    },
    "If": {
        "prefix": "if",
        "body": [
            "if ${1:condition}:",
            "    ${2:pass}"
        ],
        "description": "Creates an if expression."
    },
    "Elif": {
        "prefix": "elif",
        "body": [
            "elif ${1:condition}:",
            "    ${2:pass}"
        ],
        "description": "Creates an elif expression."
    },
    "Else": {
        "prefix": "else",
        "body": [
            "else:",
            "    ${1:pass}"
        ],
        "description": "Creates an else expression."
    },
    "If-Else-Block": {
        "prefix": "ife",
        "body": [
            "if ${1:condition}:",
            "    ${2:pass}",
            "else:",
            "    ${3:pass}"
        ],
        "description": "Creates an if/else expression."
    },
    "If-Else-Inline": {
        "prefix": "if_e",
        "body": [ "if ${1:condition}: ${2:pass} else: ${3:pass}" ],
        "description": "Creates an if/else expression."
    },
    "While": {
        "prefix": "while",
        "body": [
            "while ${1:condition}:",
            "    ${2:pass}"
        ],
        "description": "Creates a while loop."
    },
    "For": {
        "prefix": "for",
        "body": [
            "for ${1:variable} in ${2:iterable}:",
            "    ${3:pass}"
        ],
        "description": "Creates a for loop."
    },
    "If-Where": {
        "prefix": "ifw",
        "body": [
            "if ${1:condition1} where ${2:condition2}",
            "    ${3:pass}"
        ],
        "description": "Creates an if expression with a 'where' clause."
    },
    "While-Where": {
        "prefix": "whilew",
        "body": [
            "while ${1:condition1} where ${2:condition2}",
            "    ${3:pass}"
        ],
        "description": "Creates a while loop with a 'where' clause."
    },
    "For-Where": {
        "prefix": "forw",
        "body": [
            "for ${1:variable} in ${2:iterable} where ${3:codition}:",
            "    ${4:pass}"
        ],
        "description": "Creates a for loop with a 'where' clause."
    }
}
